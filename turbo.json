{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        "dist/**",
        ".next/**"
      ],
      "env": [
        "NODE_ENV",
        "PORT",
        "TEST_HOST",
        "API_KEYS"
      ]
    },
    "lint": {
      "outputs": []
    },
    "dev": {
      "dependsOn": [
        "^build"
      ],
      "cache": false
    },
    "test:unit": {
      "outputs": [],
      "cache": false
    }
  }
}
