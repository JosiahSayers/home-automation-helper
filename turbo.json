{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": ["^build", "$NODE_ENV", "$PORT", "$TEST_HOST", "$API_KEYS", "$FIREBASE_ADMIN_CREDENTIALS", "$FIREBASE_TESTING_API_KEY"],
      "outputs": ["dist/**", ".next/**"]
    },
    "lint": {
      "outputs": []
    },
    "dev": {
      "dependsOn": ["^build"],
      "cache": false
    },
    "test:unit": {
      "outputs": [],
      "cache": false
    }
  }
}
